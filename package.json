{
  "name": "v0",
  "version": "1.0.3",
  "description": "To be Willin is to be willing.",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "prepare": "npm run build && doctoc --github",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "exports": {
    ".": {
      "node": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./operators": {
      "node": "./dist/operators/index.js",
      "default": "./src/operators/index.ts"
    },
    "./decorators": {
      "node": "./dist/decorators/index.js",
      "default": "./src/decorators/index.ts"
    },
    "./utils": {
      "node": "./dist/utils/index.js",
      "default": "./src/utils/index.ts"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "reflect-metadata": ">0.1.13",
    "rxjs": ">6.6.6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@willin/eslint-config": "^0.0.6",
    "doctoc": "^2.0.0",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willin/v0.git"
  },
  "keywords": [
    "rxjs",
    "operators",
    "v0",
    "willin"
  ],
  "author": "Willin Wang",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/willin/v0/issues"
  },
  "homepage": "https://github.com/willin/v0#readme"
}
